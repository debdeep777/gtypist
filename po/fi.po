msgid "invalid function key number"
msgstr "virheellinen funktionäppäimen numero"

#: gtypist.c:629 gtypist.c:739 gtypist.c:970
msgid "data exceeds screen length"
msgstr "koko teksti ei mahdu ruudulle"

#: gtypist.c:666
msgid "instructions are limited to two lines"
msgstr "ohjeita saa olla enintään kaksi riviä"

#: gtypist.c:1466
#, no-c-format
msgid "'*' must immediately follow '%'"
msgstr "asteriskin ('*') pitää olla välittömästi '%':n jälkeen"

#: gtypist.c:1469 gtypist.c:1472
#, no-c-format
msgid "missing '%'"
msgstr "puuttuva '%'"

#: gtypist.c:1476
#, no-c-format
