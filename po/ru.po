#, c-format
msgid "%s: invalid colours value\n"
msgstr "%s: неправильная величина цвета\n"

#: src/gtypist.c:1769
#, c-format
msgid "%s:  argument format is incorrect\n"
msgstr "%s:  формат параметра неверен\n"

#: src/gtypist.c:1779
#, c-format
msgid "%s:  incorrect colour values\n"
msgstr "%s:  неверные величины цветов\n"

